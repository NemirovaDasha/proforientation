{
  "name": "builder",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "prettier": "prettier --config config/prettier.config.js --write src/scripts/*.js",
    "lint": "eslint --config config/eslint.config.js --fix src/scripts/*.js "
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
