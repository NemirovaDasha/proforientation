    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-batch": "^1.0.5",
    "gulp-exec": "^3.0.2",
    "gulp-html-beautify": "^1.0.1",
    "gulp-imagemin": "^5.0.3",
