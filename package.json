    "gulp-twig": "^1.2.0",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^5.0.1",
    "husky": "^1.1.3",
    "jquery": "^3.6.0",
    "lint-staged": "^8.0.4",
    "minimist": "^1.2.5",
    "natives": "^1.1.6",
    "prettier": "^1.14.3",
    "vue": "^2.6.10",
    "vue-notification": "^1.3.16",
    "webpack-stream": "^5.2.1"
